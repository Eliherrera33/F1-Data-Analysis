FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.3750;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.3750;
    }
    outlet
    {
        type            zeroGradient;
    }
    ground
    {
        type            kqRWallFunction;
        value           uniform 0.3750;
    }
    top
    {
        type            slip;
    }
    sides
    {
        type            slip;
    }
    car
    {
        type            kqRWallFunction;
        value           uniform 0.3750;
    }
}
