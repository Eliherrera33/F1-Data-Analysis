FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 11.18;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 11.18;
    }
    outlet
    {
        type            zeroGradient;
    }
    ground
    {
        type            omegaWallFunction;
        value           uniform 11.18;
    }
    top
    {
        type            slip;
    }
    sides
    {
        type            slip;
    }
    car
    {
        type            omegaWallFunction;
        value           uniform 11.18;
    }
}
